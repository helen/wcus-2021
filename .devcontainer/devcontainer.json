{
	"name": "${localWorkspaceFolderBasename}",
	"dockerComposeFile": "docker-compose.yml",
	"service": "wordpress",
	"forwardPorts": [ 8080 ],
	"workspaceFolder": "/var/www/html/",
	"postCreateCommand": "/var/www/html/wp-content/themes/${localWorkspaceFolderBasename}/.devcontainer/setup.sh",
	"remoteUser": "www-data"
}
